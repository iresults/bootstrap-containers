{
    "name": "iresults/bootstrap-containers",
    "description": "A `EXT:container` based replacement for `EXT:bootstrap_grids`",
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "b13/container": "^3.1",
        "twbs/bootstrap-sass": "3.*",
        "typo3/cms-core": "^11.5 || ^12.4 || ^13.4"
    },
    "autoload": {
        "psr-4": {
            "Iresults\\BootstrapContainers\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "bootstrap_containers"
        }
    }
}
